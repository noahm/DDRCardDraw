{
  "private": true,
  "name": "ddr-card-draw",
  "version": "1.0.0",
  "description": "Tools for music game tournaments and training",
  "engines": {
    "node": "^24.0.0"
  },
  "scripts": {
    "start": "webpack serve --env dev=true",
    "build": "webpack",
    "build:zip": "npm run validate && webpack --env zip",
    "deploy:surge": "surge ./surge-redirect https://ddrdraw.surge.sh",
    "deploy:staging": "surge ./surge-redirect https://ddrdraw-staging.surge.sh",
    "format": "prettier --experimental-cli --write ./src ./scripts",
    "import:ddr": "node --experimental-strip-types scripts/import-ddr.mts",
    "import:jubeat": "node --experimental-strip-types scripts/import-jubeat.mts",
    "import:pump": "node scripts/import-pump.mjs",
    "import:maimai": "node --experimental-strip-types scripts/import-maimai.mts",
    "import:sdvx": "node scripts/import-sdvx.mjs",
    "import:iidx": "node scripts/import-iidx.mjs",
    "import:smx": "node scripts/import-smx.mjs",
    "import:itg": "node scripts/import-itg.mjs",
    "import:eventList": "node scripts/event-filter.mjs",
    "validate": "npm run validate:json && npm run validate:ts && npm run validate:format && npm run validate:lint",
    "validate:lint": "eslint",
    "validate:json": "node scripts/validate.mjs",
    "validate:format": "prettier --experimental-cli --check ./src ./scripts",
    "validate:ts": "tsc --noemit"
  },
